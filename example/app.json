{
  "expo": {
    "name": "Emotion Extended", 
    "slug": "emotion-native-extended-example",
    "privacy": "public",
    "entry": "./App.tsx",
    "githubUrl": "https://github.com/ItsWendell/emotion-native-extended/tree/develop/example/",
    "platforms": [
      "ios",
      "android",  
      "web"
    ],
    "version": "1.0.0",
    "sdkVersion": "37.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    }
  }
}
